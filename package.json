{
  "name": "asr-nexus-explorer",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://scientific-research.online",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --config vitest.config.ts src/test/unit",
    "test:integration": "vitest run --config vitest.config.ts src/test/integration",
    "test:security": "vitest run --config vitest.config.ts src/test/integration/security.test.ts",
    "test:performance": "vitest run --config vitest.config.ts src/test/unit/performance",
    "test:e2e": "playwright test",
    "test:e2e:fast": "E2E_FAST=true playwright test",
    "test:e2e:smoke": "E2E_SMOKE=true playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e:smoke",
    "test:ci:fast": "npm run test:coverage && npm run test:e2e:fast",
    "test:ci:full": "npm run test:coverage && npm run test:e2e",
    "test:e2e:info": "./scripts/e2e-performance-info.sh",
    "test:e2e:perf": "./scripts/test-performance.sh",
    "test:watch": "vitest",
    "test:pre-push": "./scripts/full-test-suite.sh",
    "test:quick": "./scripts/quick-test.sh",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy:old": "node scripts/deploy.js",
    "deploy:validate": "node scripts/deploy.js validate",
    "deploy:migrate": "node scripts/deploy.js migrate",
    "deploy:build": "node scripts/deploy.js build",
    "deploy:verify": "node scripts/deploy.js verify"
  },
