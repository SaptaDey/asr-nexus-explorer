# Development Environment Configuration
VITE_NODE_ENV=development
VITE_API_BASE_URL=http://localhost:8080
VITE_ENABLE_DEBUG=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=false
VITE_MAX_GRAPH_NODES=500
VITE_MAX_API_CALLS_PER_MINUTE=30
VITE_CACHE_TTL_MINUTES=5
VITE_LOG_LEVEL=debug

# Feature flags for development
VITE_FEATURE_TREE_VISUALIZATION=false
VITE_FEATURE_ADVANCED_ANALYTICS=true
VITE_FEATURE_COLLABORATIVE_MODE=true
VITE_FEATURE_EXPORT_FUNCTIONALITY=true
VITE_FEATURE_MOBILE_PWA=false

# Development-specific settings
VITE_MOCK_API_RESPONSES=false
VITE_ENABLE_HOT_RELOAD=true
VITE_SKIP_AUTH_IN_DEV=false